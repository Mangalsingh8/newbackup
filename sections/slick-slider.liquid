<div class="new">
  <div class="slick-heading">
    {{ section.settings.heading }}
  </div>
  <div class="omm">
    <div class="slick-main">
   {% for block in section.blocks %}
    {%if block.settings.image_block != blank %}
  <div class="hello">
    <img class="img_block" src= "{{ block.settings.image_block | img_url:'master' }}" alt="" title="" width="100%" height="100%">
</div>
  {% endif %}
{% endfor %}
    </div>
  </div>
</div>
{% schema %}
{
"name": "Image Slider",
"settings": [
{
"type": "text",
"id": "heading",
"label": "Heading",
"default": "Quick links"
}
],
"blocks": [
    {
      "type": "Slider",
      "name": "t:sections.collage.blocks.image.name",
      "settings": [
{
"type": "image_picker",
"id": "image_block",
"label": "Image"
}
]
}
],
"presets":[
{
"name":"Image Slider",
"category":"custom"
}
]
}
{% endschema %}
